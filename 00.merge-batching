#!/bin/bash
#SBATCH --job-name=merge-batching
#SBATCH --partition="small"
#SBATCH --time=12:00:00
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=2
#SBATCH --output=logs/%x.out
set -euo pipefail
source .env
source venv/bin/activate

L=$1
COLLECTIONS=${@:2}

time \
python batching.py -j 1 -l $L $WARC2TEXT_DIR $WORKSPACE
